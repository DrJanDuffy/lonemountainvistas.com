{
  "version": 2,
  "framework": "nextjs",
  "regions": ["iad1"],
  "cleanUrls": true,
  "git": {
    "deploymentEnabled": {
      "main": true,
      "staging": true
    }
  },
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.lonemountainvistas.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "lonemountainvistas.com"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
